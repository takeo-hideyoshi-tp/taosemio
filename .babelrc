{
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  },
  "presets": [
    "@babel/react", "@babel/typescript", ["@babel/env", { "modules": false }]
  ],
  "plugins": [
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    "add-module-exports",
    "@babel/plugin-transform-object-assign",
    "@babel/plugin-proposal-class-properties",
    "react-require"
  ]
}
